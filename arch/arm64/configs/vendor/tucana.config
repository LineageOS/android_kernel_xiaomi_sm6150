# Camera
CONFIG_CSID_CAMERA=y
CONFIG_NO_DELAY_DMA_UNMAP_CAMERA=y
CONFIG_SOFTLED_CAMERA=y

# Device
CONFIG_MACH_XIAOMI_TUCANA=y

# Fingerprint
CONFIG_INPUT_FINGERPRINT=y
CONFIG_FINGERPRINT_FPC_FOD=y
CONFIG_FINGERPRINT_GOODIX_FOD=y

# Haptics
CONFIG_INPUT_DRV260X_HAPTICS=y
# CONFIG_LEDS_QPNP_HAPTICS is not set
# CONFIG_LEDS_QPNP_VIBRATOR_LDO is not set

# IR
CONFIG_LIRC=y
CONFIG_RC_DEVICES=y
CONFIG_IR_SPI=y

# LED
CONFIG_LEDS_AW3644=y

# Power
CONFIG_BATT_VERIFY_BY_DS28E16=y
CONFIG_BQ2597X_CHARGE_PUMP=y
CONFIG_ONEWIRE_GPIO=y
# CONFIG_SMB1355_SLAVE_CHARGER is not set
# CONFIG_SMB1390_CHARGE_PUMP_PSY is not set

# Ramoops
CONFIG_CMDLINE="cgroup_disable=pressure ramoops_memreserve=4M"

# SIM tray
CONFIG_SIMTRAY_STATUS=y

# Touchscreen
CONFIG_TOUCHSCREEN_GOODIX_GTX9886_F4=y
CONFIG_TOUCHSCREEN_ST_FTS_V521=y
CONFIG_FTS_FOD_AREA_REPORT=y
CONFIG_TOUCHSCREEN_XIAOMI_TOUCHFEATURE=y
CONFIG_TOUCHSCREEN_TDDI_DBCLK=y

# Ultrasound proxmity
CONFIG_US_PROXIMITY=y
