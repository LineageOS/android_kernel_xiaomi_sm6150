		qcom,supply-min-voltage = <1900000>;
		qcom,supply-max-voltage = <1900000>;
		qcom,supply-enable-load = <32000>;
		qcom,supply-disable-load = <80>;
		qcom,supply-post-off-sleep = <20>;
