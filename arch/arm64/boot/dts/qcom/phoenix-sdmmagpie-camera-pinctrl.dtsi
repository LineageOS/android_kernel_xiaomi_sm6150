		function = "normal";
		power-source = <1>;
		output-low;
	};

	cam_sensor_fa_shutdown_suspend: cam_sensor_fa_shutdown_suspend {
		pins = "gpio10";
		function = "normal";
		power-source = <1>;
		output-low;
	};

};

&pm6150_gpios {

	cam_sensor_mclk_fa_active: cam_sensor_mclk_fa_active {
