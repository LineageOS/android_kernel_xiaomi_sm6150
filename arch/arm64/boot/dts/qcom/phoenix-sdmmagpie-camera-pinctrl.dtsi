&pm6150l_gpios {

	/* WIDE VDDA ENABLE */
	cam_sensor_w_vdda_enable: cam_sensor_w_vdda_enable {
		pins = "gpio8";
		function = "normal";
		power-source = <1>;
		output-low;
	};

	cam_sensor_w_vdda_disable: cam_sensor_w_vdda_disable {
		pins = "gpio8";
		function = "normal";
		power-source = <1>;
		output-low;
	};

	/* WIDE VDDD & FRONT_AUX VDDD ENABLE */
	cam_sensor_w_vddd_enable: cam_sensor_w_vddd_enable {
		pins = "gpio1";
		function = "normal";
		power-source = <0>;
		output-low;
	};

	cam_sensor_w_vddd_disable: cam_sensor_w_vddd_disable {
