		output-low;
	};

	/* DEPTH/ULTRA VDDA ENABLE */
	cam_sensor_d_u_vdda_enable: cam_sensor_d_u_vdda_enable {
		pins = "gpio3";
		function = "normal";
		power-source = <0>;
