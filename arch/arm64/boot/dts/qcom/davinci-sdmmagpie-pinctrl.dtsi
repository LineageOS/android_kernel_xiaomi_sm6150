	motor_gpio_default: motor_gpio_default {
		mux {
			/* DIR, M0, M1, SLEEP, EN */
			pins = "gpio55", "gpio91", "gpio66", "gpio88", "gpio90";
			function = "gpio";
		};

		config {
			/* DIR, M0, M1, SLEEP, EN */
			pins = "gpio55", "gpio91", "gpio66", "gpio88", "gpio90";
			bias-pull-up; /* No PULL */
			drive-strength = <2>; /* 2 MA */
		};
	};

